<table class='table table-striped table-condensed small'>
  <thead>
    <tr>
      <th>Select</th>
      <th>Priority</th>
      <th>ID</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Job title</th>
      <th>Company</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% members.each do |member| %>
      <tr>
        <td><%=
            check_box_tag 'members[]', member.id, true
        %></td>
        <td><%= member.priority %></td>
        <td><%= member.id %></td>
        <td><%= member.first_name %></td>
        <td><%= member.last_name %></td>
        <td><%= member.job_title %></td>
        <td><%= member.organisation %></td>
        <td><%= member.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
